      border-bottom: 1px dotted #bbb;
    }

    .latest-list {
      display: grid;
      grid-template-columns: repeat(auto-fit, minmax(220px, 1fr));
      gap: 0.5rem 1rem;
      justify-items: start;
      align-items: start;
      min-width: 0; /* allow children to shrink for ellipsis */
    }

    .latest-item {
      background: transparent;
      border: none;
      border-radius: 0;
      padding: 0;
      text-decoration: none;
      color: inherit;
      display: flex; /* ensure child can flex and truncate */
      min-width: 0; /* allow child ellipsis in grid */
      max-width: 100%;
      overflow: hidden; /* ensure children don't overflow grid cell */
    }

    .latest-item:hover .latest-title {
      text-decoration: underline;
    }

    .latest-title {
      flex: 1 1 auto;
      min-width: 0;
      font-weight: 500;
      font-size: 1.15rem;
      line-height: 1.2;
      margin: 0;
      color: #333;
      display: block;
      white-space: nowrap;
      overflow: hidden;
      text-overflow: ellipsis;
      width: 100%;
      max-width: 100%;
    }

    .latest-meta {
      font-size: 1rem;
      color: #999;
      margin-left: 0.4rem;
    }

    .latest-meta::before {
      content: ' — ';
      color: #ccc;
    }

    /* Separator not needed for grid layout */
    .latest-list a + a::before {
      content: none;
    }

    /* Bottom inline variant (ultra subtle) */
    .latest-inline {
      display: flex;
      flex-wrap: wrap;
      justify-content: center;
      gap: 0.25rem;
      color: #9b9b9b;
      font-size: 1rem;
      margin-top: 0.5rem;
    }

    .latest-inline a {
      color: inherit;
      text-decoration: none;
    }

    .latest-inline a:hover {
      text-decoration: underline;
    }

    .latest-inline a + a::before {
      content: '·';
      color: #cfcfcf;
      margin: 0 0.5rem;
    }
    
    @media (max-width: 768px) {
      .latest-title { font-size: 1.1rem; }
      .latest-meta { font-size: 0.95rem; }
      .latest-list { grid-template-columns: 1fr; justify-items: start; }
      .latest-item { text-align: left; }
    }
  </style>

  <script is:inline>
    function loadGSAP() {
      return new Promise((resolve, reject) => {
        const script1 = document.createElement('script');
        script1.src = 'https://cdnjs.cloudflare.com/ajax/libs/gsap/3.9.1/gsap.min.js';
        script1.onload = function() {
          const script2 = document.createElement('script');
          script2.src = 'https://cdnjs.cloudflare.com/ajax/libs/gsap/3.9.1/ScrollTrigger.min.js';
          script2.onload = () => resolve();
          script2.onerror = () => reject(new Error('Failed to load ScrollTrigger'));
          document.head.appendChild(script2);
        };
        script1.onerror = () => reject(new Error('Failed to load GSAP'));
        document.head.appendChild(script1);
      });
    }

    // Initialize animations after GSAP loads
    function initGSAPAnimations() {
      if (typeof gsap === 'undefined') return;

      function shouldShowLoader() {
        if (sessionStorage.getItem('loaderShown')) {
          return false;
        }
        
        if (document.referrer) {
          const referrerDomain = new URL(document.referrer).hostname;
          const currentDomain = window.location.hostname;
          if (referrerDomain === currentDomain) {
            return false;
          }
        }
        
        return true;
      }

      if (!shouldShowLoader()) {
        const loader = document.querySelector('.loader');
        if (loader) {
          loader.style.display = 'none';
        }
        document.body.classList.remove('lock-scroll');
        return;
      }

      sessionStorage.setItem('loaderShown', 'true');

      gsap.to(".overlay", {
        clipPath: "polygon(0 0, 100% 0, 100% 0%, 0 0)",
        ease: "sine.inOut",
        duration: 2,
        stagger: {
          amount: 0.5,
        },
        force3D: true
      });

      gsap.to(".img", {
        clipPath: "polygon(0 100%, 100% 100%, 100% 0%, 0% 0%)",
        ease: "power2.inOut",
        duration: 2,
        stagger: {
          amount: 1.5,
        },
        force3D: true
      });

      gsap.to(".loader", {
        clipPath: "polygon(0% 0%, 100% 0%, 100% 0%, 0% 0%)",
        ease: "expo.inOut",
        duration: 2,
        delay: 2,
        force3D: true,
        onComplete: () => {
          document.body.classList.remove('lock-scroll');
          window.scrollTo(0, 0);
        }
      });
    }

    // Initialize ScrollTrigger animations
    function initScrollTrigger() {
      if (typeof gsap === 'undefined' || typeof ScrollTrigger === 'undefined') return;
      
      gsap.registerPlugin(ScrollTrigger);
      
      document.querySelectorAll('section').forEach(section => {
        section.classList.add('fade-in');
        ScrollTrigger.create({
          trigger: section,
          start: "top 80%",
          onEnter: () => section.classList.add('visible')
        });
      });
    }

    function shouldShowLoader() {
      if (sessionStorage.getItem('loaderShown')) {
        return false;
      }
      
      if (document.referrer) {
        const referrerDomain = new URL(document.referrer).hostname;
        const currentDomain = window.location.hostname;
        if (referrerDomain === currentDomain) {
          return false;
        }
      }
      
      return true;
    }

    if (shouldShowLoader()) {
      document.body.classList.add('lock-scroll');
    } else {
      const loader = document.querySelector('.loader');
      if (loader) {
        loader.style.display = 'none';
      }
      document.body.classList.remove('lock-scroll');
    }

    window.addEventListener('load', async function() {
      try {
        await loadGSAP();
        initGSAPAnimations();
        initScrollTrigger();
      } catch (error) {
        console.error('Failed to load GSAP:', error);
      }
    });
  </script>

