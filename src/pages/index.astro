    /* Optimize animations with hardware acceleration */
    .loader, .img, .overlay {
      will-change: transform, opacity, clip-path;
      transform: translateZ(0);
      backface-visibility: hidden;
    }

    /* Use CSS transforms instead of JavaScript where possible */
