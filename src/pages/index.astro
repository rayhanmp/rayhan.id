    /* Optimize scrollbar behavior */
    html {
      overflow-y: scroll;
      scrollbar-gutter: stable;
      scroll-behavior: smooth;
    }

    body.lock-scroll {
      overflow: hidden !important;
      height: 100vh !important;
      position: fixed !important;
